/* Positive anything is better than negative nothing*/

#include <stdio.h>

int main (){
    int n;
    printf("Enter any number:");
    scanf("%d", &n);
    if (n > 0) {
        printf("n is a positive number\n");
    } else if (n = 0) {
        printf("n is zero");
    }else if (n < 0) {
        printf("n is a negative number\n");
    }
    return 0;
}
